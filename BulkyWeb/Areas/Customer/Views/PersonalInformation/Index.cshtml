@model ComsumerInfoVM

<div class="row pb-3">

    <div class="border p-3 mt-4">
        <div class="row pb-2  text-center">
            <h2 class="text-primary">Personal Information</h2>
            <hr />
        </div>
        <div class="row pb-2">
            <p><strong>Name:</strong> @Model.ApplicationUser.Name</p>
            <p><strong>StreetAddress:</strong> @Model.ApplicationUser.StreetAddress</p>
            <p><strong>City:</strong> @Model.ApplicationUser.City</p>
            <p><strong>State:</strong> @Model.ApplicationUser.State</p>
            <p><strong>PostalCode:</strong> @Model.ApplicationUser.PostalCode</p>
            <p><strong>Purchase Points:</strong>@Model.PurchasePoint.Point</p>
            
        </div>
        




    </div>
  
    
    
</div>